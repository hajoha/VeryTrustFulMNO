<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/HomeFragment">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.HomeFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_HomeFragment_to_AboutFragment"
            app:destination="@id/about_fragment" />
        <action
            android:id="@+id/action_FirstFragment_to_SliceFragment"
            app:destination="@+id/fragment_slice" />
        <action
            android:id="@+id/action_HomeFragment_to_fragment_slicingsetup"
            app:destination="@id/fragment_slicingsetup" />
        <action
            android:id="@+id/action_HomeFragment_to_iperf3Fragment"
            app:destination="@id/fragment_iperf3_input" />
        <action
            android:id="@+id/action_HomeFragment_to_specialCodeFragment"
            app:destination="@id/specialCodesFragment" />
        <action
            android:id="@+id/action_HomeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_HomeFragment_to_subscriptionsFragment"
            app:destination="@id/subscriptionsFragment" />
        <action
            android:id="@+id/action_HomeFragment_to_ping_fragment"
            app:destination="@id/ping_fragment" />
        <action
            android:id="@+id/action_HomeFragment_to_carrierSettingsFragment"
            app:destination="@id/carrierSettingsFragment" />
        <action
            android:id="@+id/action_HomeFragment_self"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_HomeFragment_to_basicManagedProfileFragment2"
            app:destination="@id/basicManagedProfileFragment2" />
        <action
            android:id="@+id/action_HomeFragment_to_workProfileActivity"
            app:destination="@id/workProfileActivity" />
        <action
            android:id="@+id/action_HomeFragment_to_callFragment2"
            app:destination="@id/callFragment" />
        <action
            android:id="@+id/action_HomeFragment_to_addProfileInfoFragment"
            app:destination="@id/addProfileInfoFragment" />

    </fragment>

    <fragment
        android:id="@+id/about_fragment"
        android:name="de.foo.bar.VeryTrustfulMNO.AboutFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_about">
        <action
            android:id="@+id/action_about_fragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>


    <fragment
        android:id="@+id/ping_fragment"
        android:name="de.foo.bar.VeryTrustfulMNO.Ping.PingFragment"
        android:label="@string/action_ping"
        tools:layout="@layout/fragment_ping">

        <action
            android:id="@+id/action_ping_fragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>

    <fragment
        android:id="@+id/fragment_slicingsetup"
        android:name="de.foo.bar.VeryTrustfulMNO.SlicingSetupFragment"
        android:label="@string/slicing_setup"
        tools:layout="@layout/fragment_slicingsetup">
        <action
            android:id="@+id/action_fragment_slicingsetup_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragment_iperf3_input"
        android:name="de.foo.bar.VeryTrustfulMNO.Iperf3.Fragments.Iperf3Fragment"
        android:label="fragment_iperf3_input"
        tools:layout="@layout/fragment_iperf3_input">

        <action
            android:id="@+id/action_fragment_iperf3_input_to_HomeFragment"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_fragment_iperf3_input_to_runners_list"
            app:destination="@id/runners_list" />
    </fragment>

    <fragment
        android:id="@+id/flagSettingFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SettingPreferences.MobileNetworkSettingsFragment"
        android:label="FlagSettingFragment"
        tools:layout="@xml/preference_mobile_network">
        <action
            android:id="@+id/action_flagSettingFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/loggingSettingsFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SettingPreferences.LoggingSettingsFragment"
        android:label="LoggingSettingsFragment"
        tools:layout="@xml/preference_logging">
        <action
            android:id="@+id/action_loggingSettingsFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>

    <fragment
        android:id="@+id/mqttSettingsFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SettingPreferences.MQTTSettingsFragment"
        android:label="MQTTSettingsFragment"
        tools:layout="@xml/preference_mqtt">
        <action
            android:id="@+id/action_mqttSettingsFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SettingPreferences.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@xml/preference">
        <action
            android:id="@+id/action_settingsFragment2_to_flagSettingFragment"
            app:destination="@id/flagSettingFragment" />
        <action
            android:id="@+id/action_settingsFragment2_to_loggingSettingsFragment"
            app:destination="@id/loggingSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_fragment_shared_preferences_io2"
            app:destination="@id/fragment_shared_preferences_io" />
        <action
            android:id="@+id/action_settingsFragment_to_mqttSettingsFragment2"
            app:destination="@id/mqttSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_cloudSyncFragment"
            app:destination="@id/loginEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/specialCodesFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SpecialCodesFragment"
        android:label="SpecialCodeFragment">
        <action
            android:id="@+id/action_specialCodeFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/subscriptionsFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.SubscriptionsFragment"
        android:label="fragment_subscriptions"
        tools:layout="@layout/fragment_subscriptions" >
        <action
            android:id="@+id/action_subscriptionsFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/carrierSettingsFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.CarrierSettingsFragment"
        android:label="CarrierSettingsFragment" >
        <action
            android:id="@+id/action_carrierSettingsFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>

    <fragment
        android:id="@+id/fragment_shared_preferences_io"
        android:name="de.foo.bar.VeryTrustfulMNO.Preferences.SharedPreferencesIOFragment"
        android:label="SharedPreferencesIO" >
        <action
            android:id="@+id/action_fragment_shared_preferences_io_to_settingsFragment2"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/basicManagedProfileFragment2"
        android:name="de.foo.bar.VeryTrustfulMNO.WorkProfile.BasicManagedProfileFragment"
        android:label="BasicManagedProfileFragment" >
        <action
            android:id="@+id/action_basicManagedProfileFragment2_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>
    <activity
        android:id="@+id/workProfileActivity"
        android:name="de.foo.bar.VeryTrustfulMNO.WorkProfile.WorkProfileActivity"
        android:label="setup_profile_fragment"
        tools:layout="@layout/setup_profile_fragment" />
    <fragment
        android:id="@+id/callFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.CallFragment"
        android:label="fragment_call"
        tools:layout="@layout/fragment_call" >
        <action
            android:id="@+id/action_callFragment_to_HomeFragment2"
            app:destination="@id/HomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/addProfileInfoFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.WifiScanner.AddProfileInfoFragment"
        android:label="AddProfileInfoFragment"
        tools:layout="@layout/fragment_add_profile_info">
        <action
            android:id="@+id/action_addProfileInfoFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>


<fragment
android:id="@+id/loginEmailFragment"
android:name="de.foo.bar.VeryTrustfulMNO.ui.sync.LoginEmailFragment"
android:label="Cloud Sync Login"
tools:layout="@layout/fragment_login_email">
    <action
        android:id="@+id/action_loginEmailFragment_to_loginPasswordFragment"
        app:destination="@id/loginPasswordFragment">
        <argument
            android:name="email"
            app:argType="string" />
    </action>


</fragment>

<fragment
android:id="@+id/loginPasswordFragment"
android:name="de.foo.bar.VeryTrustfulMNO.ui.sync.LoginPasswordFragment"
android:label="Passwort eingeben"
tools:layout="@layout/fragment_login_password" >

<argument
    android:name="email"
    app:argType="string" />
</fragment>

    <fragment
        android:id="@+id/wificallFragment"
        android:name="de.foo.bar.VeryTrustfulMNO.WifiCallFragment"
        android:label="fragment_wifi_call" >
        <action
            android:id="@+id/action_callFragment_to_HomeFragment2"
            app:destination="@id/HomeFragment" />
    </fragment>
</navigation>